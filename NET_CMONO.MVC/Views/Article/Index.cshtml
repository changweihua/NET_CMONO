@model IEnumerable<ArticleModel>

@if(Model.Any()) 
{
    @foreach(var article in Model) 
    {
        <div class="row post-item" style="background:url(pcate/1.png) bottom right no-repeat;">
            <div class="col s12 m12 l12">
                <div class="progress"> 
                    <div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 15%;"> 

                    </div> 
                </div>
            </div>
            <div class="col s12 m12 l12">
                <h3><a class="shake-hover" style="color:#009688;" href="">@article.ArticleTitle</a></h3>
                <div>
                    @Html.Raw(Markdig.Markdown.ToHtml(article.ArticleSummary))

                    <div class="chip">
                        <img src="/img/yuna.jpg" alt="Author" /> Written by Weihua Chang
                    </div>
                </div>
            </div>
        </div>
    }
}